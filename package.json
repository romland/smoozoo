{
  "name": "smoozoo",
  "version": "1.0.0",
  "description": "This is Smoozoo - a super-smooth, performant and modern image viewer for the web",
  "scripts": {
    "prebuild": "rm -rf dist",
    "start": "parcel src/index.html --dist-dir dev",
    "build": "parcel build src/index.html --no-source-maps --public-url ./",
    "deploy": "npm run build && scripts/deploy.sh"
  },
  "author": "Joakim Romland",
  "license": "MIT",
  "devDependencies": {
    "parcel": "^2.15.4"
  }
}
