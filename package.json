{
  "name": "smoozoo",
  "private": true,
  "version": "1.0.0",
  "description": "This is Smoozoo - a super-smooth, performant and modern image viewer for the web",
  "scripts": {
    "prebuild": "rm -rf dist",

    "start": "npx parcel src/examples/*.html --dist-dir dev",
    "starttauri": "npx parcel ../smoozoo-native/src-web/index.html --dist-dir dev",

    "build": "npx parcel build src/examples/index.html --no-source-maps --public-url ./",
    "buildtauri": "npx parcel build ../smoozoo-native/src-web/index.html --no-source-maps --public-url ./",
    "buildminimal": "npm run prebuild && npx parcel build src/examples/minimal.html --no-source-maps --no-content-hash --public-url ./ --no-scope-hoist",
    "buildall": "npx parcel build src/examples/*.html --no-source-maps --public-url ./",

    "deploy": "npm run build && scripts/deploy.sh",
    "deployall": "npm run buildall && scripts/deploy.sh"
  },
  "author": "Joakim Romland",
  "license": "AGPL",
  "devDependencies": {
    "@parcel/config-default": "2.15.4",
    "@parcel/resolver-default": "2.15.4",
    "@parcel/transformer-raw": "2.15.4",
    "parcel": "2.15.4"
  }
}
