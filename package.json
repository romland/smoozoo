{
  "name": "smoozoo",
  "private": true,
  "version": "1.0.0",
  "description": "This is Smoozoo - a super-smooth, performant and modern image viewer for the web",
  "scripts": {
    "start": "parcel src/examples/*.html --dist-dir dev",
    "prebuild": "rm -rf dist",
    "build": "parcel build src/examples/index.html --no-source-maps --public-url ./",
    "buildall": "parcel build src/examples/*.html --no-source-maps --public-url ./",
    "deploy": "npm run build && scripts/deploy.sh",
    "deployall": "npm run buildall && scripts/deploy.sh"
  },
  "author": "Joakim Romland",
  "license": "AGPL",
  "devDependencies": {
    "@parcel/transformer-raw": "^2.15.4",
    "parcel": "^2.15.4"
  }
}
